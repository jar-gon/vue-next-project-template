{
  "name": "vue-next-project-template",
  "version": "1.0.0",
  "description": "一套基于vue3、element-plus、typesScript4、vite2的后台集成方案",
  "private": false,
  "scripts": {
    "i": "yarn install",
    "dev": "vite --mode base",
    "build:dev": "vue-tsc --noEmit && vite build --mode dev",
    "serve": "vite preview",
    "check": "npx npm-check-updates",
    "check:update": "npx npm-check-updates -u",
    "clean:main": "npx rimraf node_modules",
    "lint": "eslint --fix --ext .js,.ts,.vue ./src",
    "format": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,vue,html,md}\"",
    "lint:style": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "postinstall": "husky install",
    "ts:check": "vue-tsc --noEmit",
    "fat": "vue-tsc --noEmit && vite build --mode fat",
    "uat": "vue-tsc --noEmit && vite build --mode uat",
    "pro": "vue-tsc --noEmit && vite build --mode pro"
  },
  "dependencies": {
    "@element-plus/icons": "^0.0.11",
    "axios": "^0.24.0",
    "clipboard": "^2.0.8",
    "echarts": "^5.2.2",
    "echarts-wordcloud": "^2.0.0",
    "element-plus": "1.2.0-beta.3",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^6.2.0",
    "pinia": "^2.0.4",
    "qs": "^6.10.1",
    "screenfull": "^6.0.0",
    "vue": "^3.2.16",
    "vue-router": "^4.0.11",
    "web-storage-cache": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/intro.js": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^1.10.0",
    "@vitejs/plugin-vue-jsx": "^1.3.0",
    "autoprefixer": "^10.4.0",
    "commitizen": "^4.2.4",
    "eslint": "8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.1.4",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "less": "^4.1.2",
    "lint-staged": "^12.1.2",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.2",
    "rimraf": "^3.0.2",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.2",
    "unplugin-element-plus": "^0.1.3",
    "unplugin-vue-components": "^0.17.2",
    "vite": "^2.6.14",
    "vite-plugin-commonjs-externals": "^0.1.1",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-svg-icons": "^1.0.5",
    "vite-plugin-vue-setup-extend": "^0.1.0",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.29.6"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 3.0.0"
  }
}
